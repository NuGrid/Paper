  date-added    = {2012-12-03 03:39:18 +0000},
  date-modified = {2012-12-03 03:39:18 +0000},
  doi           = {10.1051/0004-6361:20020755},
  eprint        = {arXiv:astro-ph/0205370},
  keywords      = {nuclear reactions, nucleosynthesis, abundances, stars: interiors, stars: evolution, stars: rotation, stars: early-types, stars: AGB and post-AGB},
  url           = {http://adsabs.harvard.edu/cgi-bin/nph-bib_query?bibcode=2002A%26A...390..561M&db_key=AST},
}

